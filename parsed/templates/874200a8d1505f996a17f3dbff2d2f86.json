{"components/cart/coupon-input":"<div class=\"cart-total-value\">\n\n    <button class=\"coupon-code-add\">{{lang 'cart.coupons.add_coupon'}}</button>\n\n    <button class=\"coupon-code-cancel\" style=\"display: none;\">{{lang 'cart.coupons.cancel'}}</button>\n</div>\n\n<div class=\"cart-form coupon-code\" style=\"display: none;\">\n    <form class=\"form form--hiddenLabels coupon-form\" method=\"post\" action=\"{{urls.cart}}\">\n        <label class=\"form-label\" for=\"couponcode\">{{lang 'cart.coupons.coupon_code'}}</label>\n        <input class=\"form-input\" data-error=\"{{lang 'cart.coupons.empty_error'}}\"  id=\"couponcode\" type=\"text\" name=\"couponcode\" value=\"\" placeholder=\"{{lang 'cart.coupons.coupon_code'}}\">\n        <input class=\"button button--primary button--small\" type=\"submit\" value=\"{{lang 'cart.coupons.button'}}\">\n        <input type=\"hidden\" name=\"action\" value=\"applycoupon\">\n    </form>\n</div>\n","components/cart/gift-certificate-input":"<div class=\"cart-total-value\">\n\n    <button class=\"gift-certificate-add\">{{lang 'cart.gift_certificates.gift_certificate'}}</button>\n\n    <button class=\"gift-certificate-cancel\" style=\"display: none;\">{{lang 'cart.coupons.cancel'}}</button>\n</div>\n\n<div class=\"cart-form gift-certificate-code\" style=\"display: none;\">\n    <form class=\"form form--hiddenLabels cart-gift-certificate-form\" method=\"post\" action=\"{{urls.cart}}\">\n        <label class=\"form-label\" for=\"certcode\">{{lang 'cart.gift_certificates.cert_code'}}</label>\n        <input class=\"form-input\" id=\"certcode\" type=\"text\" name=\"certcode\" value=\"\" placeholder=\"{{lang 'cart.gift_certificates.add_cert_code'}}\">\n        <input class=\"button button--primary button--small\" type=\"submit\" value=\"{{lang 'cart.coupons.button'}}\">\n        <input type=\"hidden\" name=\"action\" value=\"applycoupon\">\n    </form>\n</div>\n","components/cart/shipping-estimator":"{{inject 'shippingCountryErrorMessage' (lang 'cart.shipping_estimator.empty_country_error')}}\n{{inject 'shippingProvinceErrorMessage' (lang 'cart.shipping_estimator.empty_province_error')}}\n\n{{#if shipping_cost}}\n    <div class=\"cart-total-value\">\n        <div class=\"subtotal shipping-estimate-show\">\n            <a href=\"#\" class=\"shipping-estimate-value\">{{shipping_cost.formatted}}</a>\n        </div>\n    </div>\n{{else}}\n    <div class=\"cart-total-value\">\n        <button data-collapsible=\"add-shipping\" class=\"shipping-estimate-show\" aria-labelledby=\"estimator-add\">\n            <span class=\"shipping-estimate-show__btn-name\">{{lang 'cart.shipping_estimator.add_info'}}</span>\n            <span id=\"estimator-add\" class=\"u-hidden\">{{lang 'cart.shipping_estimator.add_info'}}</span>\n            <span id=\"estimator-close\" class=\"u-hidden\">{{lang 'cart.shipping_estimator.cancel'}}</span>\n        </button>\n    </div>\n{{/if}}\n\n<div id=\"add-shipping\" class=\"shipping-estimator u-hidden\">\n    <form class=\"form estimator-form\" data-shipping-estimator>\n        <dl>\n            <dt class=\"estimator-form-label\">\n                <label class=\"form-label\" for=\"shipping-country\">{{lang 'cart.shipping_estimator.select_a_country'}}</label>\n            </dt>\n\n            <dd class=\"estimator-form-input\">\n                <select class=\"form-select\" id=\"shipping-country\" name=\"shipping-country\" data-field-type=\"Country\">\n                    <option>{{lang 'cart.shipping_estimator.select_a_country'}}</option>\n                    {{#each countries}}\n                        <option value=\"{{id}}\" {{#if selected}}selected=\"selected\"{{/if}}>\n                            {{name}}\n                        </option>\n                    {{/each}}\n                </select>\n            </dd>\n\n            <dt class=\"estimator-form-label\">\n                <label class=\"form-label\" for=\"shipping-state\">{{lang 'cart.shipping_estimator.select_a_state'}}</label>\n            </dt>\n\n            <dd class=\"estimator-form-input\">\n                {{#if states.length}}\n                    <select class=\"form-select\" id=\"shipping-state\" name=\"shipping-state\" data-field-type=\"State\">\n                        <option>{{lang 'cart.shipping_estimator.select_a_state'}}</option>\n                        {{#each states}}\n                            <option value=\"{{id}}\" {{#if selected}}selected=\"selected\"{{/if}}>\n                                {{name}}\n                            </option>\n                        {{/each}}\n                    </select>\n                {{else}}\n                    <input class=\"form-input\" type=\"text\" id=\"shipping-state\" name=\"shipping-state\" data-field-type=\"State\" placeholder=\"{{lang 'cart.shipping_estimator.select_a_state'}}\">\n                {{/if}}\n            </dd>\n\n            <dt class=\"estimator-form-label\">\n                <label class=\"form-label\" for=\"shipping-city\">{{lang 'cart.shipping_estimator.suburb_city'}}</label>\n            </dt>\n            <dd class=\"estimator-form-input\">\n                <input class=\"form-input\" type=\"text\" id=\"shipping-city\" name=\"shipping-city\" value=\"{{selected_city}}\" placeholder=\"{{lang 'cart.shipping_estimator.suburb_city'}}\">\n            </dd>\n\n            <dt class=\"estimator-form-label\">\n                <label class=\"form-label\" for=\"shipping-zip\">{{lang 'cart.shipping_estimator.zip_postal_code'}}</label>\n            </dt>\n            <dd class=\"estimator-form-input\">\n                <input class=\"form-input\" type=\"text\" id=\"shipping-zip\" name=\"shipping-zip\" value=\"{{selected_zip}}\" placeholder=\"{{lang 'cart.shipping_estimator.zip_postal_code'}}\">\n            </dd>\n            <button class=\"button button--primary button--small shipping-estimate-submit\">{{lang 'cart.shipping_estimator.estimate_shipping'}}</button>\n        </dl>\n    </form>\n    <div class=\"shipping-quotes\" role=\"alert\" aria-atomic=\"true\" aria-live=\"assertive\"></div>\n</div>\n","components/cart/totals":"<ul class=\"cart-totals\">\r\n     {{#each cart.items}}\r\n            \r\n            {{#inArray (split \"133,134,135,136,137,138,139,140,141,142,143,144,145,146\" \",\") (JSONstringify product_id) }}\r\n                \r\n                <style>\r\n                    .show-hide{display:flex;}\r\n                </style>\r\n                \r\n            {{/inArray}}\r\n            \r\n            {{#inArray (split \"125,127,189,128,129,131,132,191\" \",\") (JSONstringify product_id) }}\r\n                \r\n                <style>\r\n                    .warranty-hide{display:flex;}\r\n                </style>\r\n                \r\n            {{/inArray}}\r\n             {{#if product_id '==' 147}}\r\n                \r\n                <style>\r\n                    .installtion{display:none;}\r\n                </style>\r\n                \r\n            {{/if}}\r\n            {{#if product_id '==' 148}}\r\n                \r\n                <style>\r\n                    .disposal{display:none;}\r\n                </style>\r\n                \r\n            {{/if}}\r\n            {{#if product_id '==' 179}}\r\n                \r\n                <style>\r\n                    .disconnection{display:none;}\r\n                </style>\r\n                \r\n            {{/if}}\r\n            {{#if product_id '==' 198}}\r\n                \r\n                <style>\r\n                    .warranty-hide{display:none;}\r\n                </style>\r\n                \r\n            {{/if}}\r\n        {{/each}}\r\n        \r\n           <h3 class=\"help-heading show-hide\">Help with connection and removal</h3>\r\n                <li class=\"d-flex show-hide installtion\">\r\n                    <div class=\"product-icon\">\r\n                        <img src=\"https://cdn11.bigcommerce.com/s-vbvjr2ffm9/images/stencil/original/image-manager/wrench.png\">\r\n                    </div>\r\n                    <div class=\"description\">\r\n                        <h6>Connection £25 </h6>\r\n                        <p>Our professional team will unpack, connect and test your new washing machine on delivery.</p>\r\n                    </div>\r\n                    <div class=\"button cart-button\">\r\n                        <a href=\"https://checkout.ebac.com/cart.php?action=add&product_id=147\">Add to basket</a>\r\n                    </div>\r\n                    \r\n                </li>\r\n                \r\n                <li class=\"d-flex show-hide disposal\">\r\n                    <div class=\"product-icon\">\r\n                        <img src=\"https://cdn11.bigcommerce.com/s-vbvjr2ffm9/images/stencil/original/image-manager/dustbin.png\">\r\n                    </div>\r\n                    <div class=\"description\">\r\n                        <h6>Remove \\ Disposal £35.00</h6>\r\n                        <p>Our professional delivery team will remove your disconnected washing machine and take it away for recycling. Please ensure your machine is disconnected or our team will be unable to remove it.</p>\r\n                    </div>\r\n                    <div class=\"button cart-button\">\r\n                        <a href=\"https://checkout.ebac.com/cart.php?action=add&product_id=148\">Add to basket</a>\r\n                    </div>\r\n                </li>\r\n               \r\n                <li class=\"d-flex show-hide disconnection\">\r\n                    <div class=\"product-icon\">\r\n                        <img src=\"https://cdn11.bigcommerce.com/s-vbvjr2ffm9/images/stencil/original/image-manager/wrench.png\">\r\n                    </div>\r\n                    <div class=\"description\">\r\n                        <h6>Disconnection £25</h6>\r\n                        <p>Our team will disconnect your old washing machine from the plumbing system.</p>\r\n                    </div>\r\n                    <div class=\"button cart-button\">\r\n                        <a href=\"https://checkout.ebac.com/cart.php?action=add&product_id=179\">Add to basket</a>\r\n                    </div>\r\n                </li>\r\n            <h3 class=\"help-heading warranty-hide\">Warranty Upgrade</h3>\r\n            <li class=\"d-flex warranty-hide warranty-upgrade\">\r\n                <div class=\"product-icon\">\r\n                    <img src=\"https://cdn11.bigcommerce.com/s-vbvjr2ffm9/images/stencil/original/image-manager/wrench.png\">\r\n                </div>\r\n                <div class=\"description\">\r\n                    <h6>Dehumidifer Warranty Upgrade £45 </h6>\r\n                    <p>Upgrade your dehumidifier to a 5 year warranty.</p>\r\n                </div>\r\n                <div class=\"button cart-button\">\r\n                    <a href=\"https://checkout.ebac.com/cart.php?action=add&product_id=198\">Add to basket</a>\r\n                </div>\r\n                \r\n            </li>\r\n    <li class=\"cart-total\">\r\n        <div class=\"cart-total-label\">\r\n            <strong>{{lang 'cart.checkout.subtotal'}}:</strong>\r\n        </div>\r\n        <div class=\"cart-total-value\">\r\n            {{#or customer (if theme_settings.restrict_to_login '!==' true)}}\r\n                <span>{{cart.sub_total.formatted}}</span>\r\n            {{else}}\r\n                {{> components/common/login-for-pricing}}\r\n            {{/or}}\r\n        </div>\r\n    </li>\r\n    {{#if cart.gift_wrapping_cost.value}}\r\n        <li class=\"cart-total\">\r\n            <div class=\"cart-total-label\">\r\n                <strong>{{lang 'cart.checkout.gift_wrapping'}}:</strong>\r\n            </div>\r\n            <div class=\"cart-total-value\">\r\n                <span>{{cart.gift_wrapping_cost.formatted}}</span>\r\n            </div>\r\n        </li>\r\n    {{/if}}\r\n    {{#if cart.shipping_handling.show_estimator}}\r\n        <li class=\"cart-total\">\r\n            <div class=\"cart-total-label\">\r\n                <strong>{{lang 'cart.checkout.shipping'}}:</strong>\r\n            </div>\r\n            {{> components/cart/shipping-estimator cart.shipping_handling}}\r\n        </li>\r\n    {{/if}}\r\n    {{#each cart.taxes}}\r\n        {{#unless included}}\r\n            <li class=\"cart-total\">\r\n                <div class=\"cart-total-label\">\r\n                    <strong>{{name}}:</strong>\r\n                </div>\r\n                <div class=\"cart-total-value\">\r\n                    <span>{{cost.formatted}}</span>\r\n                </div>\r\n            </li>\r\n        {{/unless}}\r\n    {{/each}}\r\n    {{#if cart.discount }}\r\n        <li class=\"cart-total\">\r\n            <div class=\"cart-total-label\">\r\n                <strong>{{lang 'cart.discount'}}:</strong>\r\n            </div>\r\n            <div class=\"cart-total-value\">\r\n                {{cart.discount.formatted}}\r\n            </div>\r\n        </li>\r\n    {{/if}}\r\n    {{#if cart.coupons}}\r\n        {{#each cart.coupons}}\r\n            <li class=\"cart-total\">\r\n                <div class=\"cart-total-label\">\r\n                    <strong>{{lang 'cart.coupons.code_label' code=code}}</strong>\r\n                    <div>\r\n                        <a href=\"{{remove_url}}\">{{lang 'cart.coupons.remove'}}</a>\r\n                    </div>\r\n                </div>\r\n                <div class=\"cart-total-value\">\r\n                    <span>{{discount.formatted}}</span>\r\n                </div>\r\n            </li>\r\n        {{/each}}\r\n    {{else}}\r\n        <li class=\"cart-total\">\r\n            <div class=\"cart-total-label\">\r\n                <strong>{{lang 'cart.coupon_code'}}:</strong>\r\n            </div>\r\n            {{> components/cart/coupon-input}}\r\n        </li>\r\n    {{/if}}\r\n    {{#if settings.gift_certificates_enabled}}\r\n        {{#each cart.gift_certificates}}\r\n            <li class=\"cart-total\">\r\n                <div class=\"cart-total-label\">\r\n                    <strong>{{lang 'cart.gift_certificates.code_label' code=code}}</strong>\r\n                    <span>\r\n                        {{remaining.formatted}} | <a href=\"{{remove_url}}\">{{lang 'cart.gift_certificates.remove'}}</a>\r\n                    </span>\r\n                </div>\r\n                <div class=\"cart-total-value\">\r\n                    {{used.formatted}}\r\n                </div>\r\n            </li>\r\n        {{/each}}\r\n        <li class=\"cart-total\">\r\n            <div class=\"cart-total-label\">\r\n                <strong>{{lang 'cart.gift_certificates.gift_certificate'}}:</strong>\r\n            </div>\r\n            {{> components/cart/gift-certificate-input}}\r\n        </li>\r\n    {{/if}}\r\n    <li class=\"cart-total\">\r\n        <div class=\"cart-total-label\">\r\n            <strong>{{lang 'cart.checkout.grand_total'}}:</strong>\r\n        </div>\r\n        <div class=\"cart-total-value cart-total-grandTotal\">\r\n            {{#or customer (if theme_settings.restrict_to_login '!==' true)}}\r\n                <span>{{cart.grand_total.formatted}}</span>\r\n            {{else}}\r\n                {{> components/common/login-for-pricing}}\r\n            {{/or}}\r\n        </div>\r\n    </li>\r\n    {{#each cart.taxes}}\r\n        {{#if included}}\r\n            <li class=\"cart-total\">\r\n                <div class=\"cart-total-label\">\r\n                    <strong>{{concat name (lang 'cart.included_in_total')}}:</strong>\r\n                </div>\r\n                <div class=\"cart-total-value\">\r\n                    <span>{{cost.formatted}}</span>\r\n                </div>\r\n            </li>\r\n        {{/if}}\r\n    {{/each}}\r\n</ul>\r\n","components/common/login-for-pricing":"<p translate>{{lang 'common.login_for_pricing'}}</p>\n"}
